<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] MoneyWise</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- SB Admin 2 CSS -->
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/css/dark-mode.css" rel="stylesheet" />

    @RenderSection("Styles", required: false)
</head>
<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
       @* @Html.Partial("_Sidebar") *@
        <partial name="_Sidebar"/>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

      


            <!-- Main Content -->
            <div id="content">
                <!-- Topbar -->
                @* @Html.Partial("_Topbar") *@
                <partial name="_Topbar"/>

                <!-- End of Topbar -->
                @* @Html.Partial("_CookieExpireModal") *@
                <partial name="_CookieExpireModal" />
                  <partial name="_SavingsModal" />
                <!-- Page Content -->
                <div class="container-fluid">
                    @RenderBody()
                </div>

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            @* @Html.Partial("_Footer") *@
            <partial name="_Footer" />
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Include the Savings Modal -->
  

    <!-- JS Scripts -->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/vendor/chart.js/Chart.min.js"></script>
    <script src="~/js/sb-admin-2.min.js"></script>
    <script src="~/js/demo/Charts.js"></script>
    <script src="~/js/demo/chart-pie-demo.js"></script>
    <script src="~/js/ExpireCookieModal.js"></script>
    <script src="~/js/SavingsModal.js"></script>
    <script src="~/js/SavingsCalculator.js"></script>
    <script src="~/js/theme-toggle.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>